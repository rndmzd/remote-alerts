{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["dom", "esnext"],
      "allowJs": true,
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  